{
  "editor.formatOnSave": true,
  "workbench.iconTheme": "vscode-icons",
  "vsicons.presets.nestjs": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "files.exclude": {
    "**/dist": true,
    // "**/node_modules": true,
    "**/.cache": true,
    "**/.mongodb": true,
    "**/.redis": true,
  },
  "files.associations": {
    "**/.ebextensions/*": "yaml",
    "**/*.env*": "dotenv"
  },
  "search.exclude": {
    "dist/**": true,
    "node_modules/**": true
  },
  "vsicons.associations.files": [
    {
      "icon": "tsconfig",
      "extensions": ["tsconfig.build.json"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "typescriptdef",
      "extensions": ["interface.ts"]
    },
    {
      "icon": "nest_guard_ts",
      "extensions": ["strategy.ts"]
    },
    {
      "icon": "mongo",
      "extensions": ["model.ts"]
    }
  ],
  "vsicons.associations.folders": [
    {
      "icon": "app",
      "extensions": ["apps"],
      "format": "svg"
    }
  ]
}